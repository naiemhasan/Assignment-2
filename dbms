select G_id , g_year , g_date from Guided;


select* from guided
where t_id is '19FCS003';


Select city, s_name, state,mark, s_id,course From students;


Select mark, s_name from students ORDER BY mark DESC;

Select s_name, city from students where (state is 'ASSAM' or state is 'DELHI') and mark>7;

Select* from Students where mark<=8;

Select s_name, S_id from Students where mark>=8;



create table employee (
id int primary key not null,
department_id int,
gender varchar(1),
e_name varchar(10),
salary real,
e_id int unique
);

