create TABLE STUDENTS (
	S_id varchar(10) PRIMARY KEY NOT NULL,
    S_name varchar(225),
    Course varchar(225),
    City varchar(225),
    State varchar(225),
    Mark real
);

CREATE TABLE TEACHERS (
	T_id varchar(10) primary key not null,
    t_name varchar(255),
    city varchar(255),
    state varchar(255)
);

create table guided (
	G_id varchar(10) primary key not null,
    G_date date,
    G_year int,
    s_id varchar(10),
    t_id varchar(10),
    foreign key(s_id) references students(s_id),
    foreign key(t_id) references teachers(t_id)
);


